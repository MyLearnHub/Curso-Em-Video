Algoritmo "soma"
Var
   value1, value2: inteiro
Procedimento sum(number1, number2: inteiro)
Inicio
   Escreval("Recebi o primeiro número: ", number1)
   Escreval("Recebi o segundo número: ", number2)
   Escreval("A soma entre eles é: ", number1 + number2)
FimProcedimento
Inicio
   value1 <- 5
   value2 <- 3
   sum(value1, value2)
Fimalgoritmo